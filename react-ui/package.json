{
  "name": "kewen-lab-client",
  "version": "0.1.0",
  "private": true,
  "description": "A client for the Kewen Lab application.",
  "author": {
    "name": "Tim Astier",
    "url": "https://github.com/TimAstier"
  },
  "scripts": {
    "build-css": "node-sass src/ -o src/",
    "watch-css": "npm run build-css && node-sass src/ -o src/ --watch --recursive",
    "start-js": "react-scripts start",
    "start": "npm-run-all -p watch-css start-js",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "lint": "eslint *.js .; exit 0",
    "lint:watch": "watch 'npm run lint'"
  },
  "dependencies": {
    "axios": "^0.15.3",
    "classnames": "^2.2.5",
    "immutable": "^3.8.1",
    "jwt-decode": "^2.1.0",
    "lodash": "^4.17.4",
    "react": "^15.4.2",
    "react-dnd": "^2.3.0",
    "react-dnd-html5-backend": "^2.3.0",
    "react-dom": "^15.4.2",
    "react-redux": "^5.0.2",
    "react-router": "^3.0.1",
    "react-router-redux": "^4.0.8",
    "redux": "^3.6.0",
    "redux-devtools-extension": "^2.13.0",
    "redux-immutable": "^3.1.0",
    "redux-thunk": "^2.1.0",
    "semantic-ui-react": "^0.64.0",
    "shortid": "^2.2.6",
    "validator": "^6.2.1"
  },
  "devDependencies": {
    "node-sass": "^4.5.3",
    "npm-run-all": "^4.0.2",
    "react-scripts": "0.9.4"
  }
}
